  bibleVerseId            Int
  prophecyAboutJesusRef   ProphecyAboutJesusRef? @relation(fields: [prophecyAboutJesusRefId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  bibleVerse              BibleVerse?            @relation(fields: [bibleVerseId], references: [id])

  @@index([bibleVerseId])
  @@index([prophecyAboutJesusRefId])
}

model Question {
  id       Int            @id @default(autoincrement())
  ord      Int            @default(1)
  question String         @db.Text
  refs     QuestionRef[]
  hints    QuestionHint[]
}

model QuestionHint {
  id         Int               @id @default(autoincrement())
  hint       String            @db.Text
  questionId Int?
  question   Question?         @relation(fields: [questionId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  refs       QuestionHintRef[]

  @@index([questionId])
}

model QuestionHintRef {
  id             Int                 @id @default(autoincrement())
  stringRef      String?
  questionHintId Int
  questionHint   QuestionHint?       @relation(fields: [questionHintId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  refIds         QuestionHintRefId[]

  @@index([questionHintId])
}

model QuestionHintRefId {
  id                Int              @id @default(autoincrement())
  questionHintRefId Int
  bibleVerseId      Int
  questionHintRef   QuestionHintRef? @relation(fields: [questionHintRefId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  bibleVerse        BibleVerse?      @relation(fields: [bibleVerseId], references: [id])

  @@index([bibleVerseId])
  @@index([questionHintRefId])
}

model QuestionRef {
  id         Int             @id @default(autoincrement())
  stringRef  String?
  questionId Int
  question   Question?       @relation(fields: [questionId], references: [id], onDelete: Cascade)
  refIds     QuestionRefId[]

  @@index([questionId])
}

model QuestionRefId {
  id            Int          @id @default(autoincrement())
  questionRefId Int
  bibleVerseId  Int
  question      QuestionRef? @relation(fields: [questionRefId], references: [id], onDelete: Cascade)
  bibleVerse    BibleVerse?  @relation(fields: [bibleVerseId], references: [id])

  @@index([bibleVerseId])
  @@index([questionRefId])
}

model Quote {
  id    Int             @id @default(autoincrement())
  short String
  refs  QuoteRefGroup[]
}

model QuoteRefGroup {
  quoteId   Int
  testament Testament
  stringRef String?
  quote     Quote?            @relation(fields: [quoteId], references: [id], onDelete: Cascade)
  refIds    QuoteRefGroupId[]

  @@id([quoteId, testament])
  @@index([quoteId, testament])
}

model QuoteRefGroupId {
  id           Int            @id @default(autoincrement())
  refGroupId   Int
  bibleVerseId Int
  testament    Testament
  refGroup     QuoteRefGroup? @relation(fields: [refGroupId, testament], references: [quoteId, testament])
  bibleVerse   BibleVerse?    @relation(fields: [bibleVerseId], references: [id])

  @@index([bibleVerseId])
  @@index([refGroupId, testament])
}

model Symbol {
  id     Int          @id @default(autoincrement())
  symbol String
  hints  SymbolHint[]
}

model SymbolHint {
  id       Int             @id @default(autoincrement())
  hint     String          @db.Text
  symbolId Int
  symbol   Symbol?         @relation(fields: [symbolId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  refs     SymbolHintRef[]

  @@index([symbolId])
}

model SymbolHintRef {
  id           Int               @id @default(autoincrement())
  stringRef    String?
  symbolHintId Int?
  symbolHint   SymbolHint?       @relation(fields: [symbolHintId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  refIds       SymbolHintRefId[]

  @@index([symbolHintId])
}

model SymbolHintRefId {
  id              Int           @id @default(autoincrement())
  symbolHintRefId Int
  bibleVerseId    Int
  symbolRef       SymbolHintRef @relation(fields: [symbolHintRefId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  bibleVerse      BibleVerse?   @relation(fields: [bibleVerseId], references: [id])

  @@index([bibleVerseId])
  @@index([symbolHintRefId])
}

