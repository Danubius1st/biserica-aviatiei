{
  "name": "studiind-biblia",
  "version": "2.0.19.01",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf .next .webpack_cache node_modules/.cache",
    "clean-prisma-generated": "rimraf ./prisma/generated/prisma-db-app/ ./prisma/generated/prisma-db-auth/",
    "generate": "npm run clean-prisma-generated && npx prisma generate --schema=prisma/db-auth/schema.prisma && npx prisma generate --schema=prisma/db-app/schema.prisma",
    "generate-accelerate.old": "npx prisma generate --accelerate",
    "generate-accelerate": "npm run generate --accelerate",
    "postinstall": "npm run generate",
    "push-db-auth": "npx prisma db push --schema=prisma/db-auth/schema.prisma",
    "push-db-auth-drizzle": "drizzle-kit push:pg",
    "dev.planetscale": "npm run generate-accelerate && next dev -p 3100",
    "dev": "SET NODE_OPTIONS='--inspect' && next dev  -p 3100",
    "dev.local": "npm run generate && npm run dev",
    "build": "npm run clean && next build --turbopack",
    "build.planetscale": "npm run generate-accelerate && next build --turbopack",
    "build.local": "npm run generate && npm run build --verbose",
    "vercel-build": "npx next build --turbopack",
    "analyze": "ANALYZE_BUNDLE=true next build.local",
    "analyze:dev": "ANALYZE_BUNDLE=true next dev.local",
    "migrate:deploy": "prisma migrate deploy",
    "db:auth:migrate": "prisma migrate dev --schema=./prisma/db-auth/schema.prisma",
    "start": "next start -p 3100",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test:e2e": "playwright test",
    "counter": "node ./scripts/counter-reset.ts",
    "id-reorder": "node ./scripts/id-reorder.js",
    "update-version": "ts-node ./scripts/update-version.ts",
    "chapters-count": "tsx ./scripts/chapters-count.ts",
    "update-bible-data": "tsx ./scripts/update-bible-data.ts",
    "update-browserslist": "npx update-browserslist-db@latest",
    "seed": "tsx ./scripts/seed.ts",
    "refresh-baseline-browser-mapping": "npm i -D baseline-browser-mapping@latest"
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@hookform/resolvers": "^5.2.2",
    "@pothos/core": "^4.10.0",
    "@pothos/plugin-prisma": "^4.12.0",
    "@pothos/plugin-relay": "^4.6.2",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.554.0",
    "next": "^15.5.7",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.11",
    "react": "19.2.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.30.0",
    "rxjs": "^7.8.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.4",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-detect-offline": "^2.4.6",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "prisma": "^6.19.0",
    "resend": "^6.5.2",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "browserslist": [
    "chrome >= 113",
    "firefox >= 113",
    "safari >= 16.5"
  ]
}
